{
    "version": 2,
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": {
                "Content-Type": {
                    ".css": "text/css",
                    ".js": "application/javascript"
                }
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}